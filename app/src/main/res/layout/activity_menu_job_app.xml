            android:layout_width="260dp"
            android:layout_height="37dp"
            android:hint="Date"
            android:id="@+id/editText12"
            android:background="@color/white"
            android:layout_marginEnd="9dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/editText11"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/button2"
            android:layout_marginStart="13dp" app:layout_constraintStart_toEndOf="@+id/textView7"
            android:editable="false"/>
