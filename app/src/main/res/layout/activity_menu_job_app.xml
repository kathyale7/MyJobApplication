            android:layout_height="36dp"
            android:hint="Last Name" android:id="@+id/editText" android:layout_weight="1"
            android:background="@color/white"
            android:layout_marginEnd="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textInputEditText"
            android:layout_marginBottom="30dp"
            app:layout_constraintBottom_toTopOf="@+id/editText2"
            android:editable="false" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"/>
    <com.google.android.material.textfield.TextInputEditText
            android:layout_width="275dp"
            android:layout_height="48dp"
            android:hint="Street Address"
            android:id="@+id/editText2"
            android:background="@color/white" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textInputEditText"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="3dp"
            app:layout_constraintBottom_toTopOf="@+id/editText3" android:enabled="false"/>
    <com.google.android.material.textfield.TextInputEditText
            android:layout_width="276dp"
            android:layout_height="11dp"
            android:hint="Street Address Line 2"
            android:id="@+id/editText3"
            android:background="@color/white" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="124dp"
            android:layout_marginTop="3dp"
            app:layout_constraintTop_toBottomOf="@+id/editText2" android:layout_marginBottom="7dp"
            app:layout_constraintBottom_toTopOf="@+id/editText4" android:layout_marginEnd="9dp"
            app:layout_constraintEnd_toEndOf="parent" android:editable="false"/>
