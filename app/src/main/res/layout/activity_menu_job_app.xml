    <com.google.android.material.textfield.TextInputEditText
            android:layout_width="134dp"
            android:layout_height="37dp"
            android:hint="Area Code"
            android:id="@+id/editText9"
            android:background="@color/white" android:layout_marginStart="12dp"
            app:layout_constraintStart_toEndOf="@+id/textView5" app:layout_constraintEnd_toStartOf="@+id/editText10"
            android:layout_marginTop="6dp" app:layout_constraintTop_toBottomOf="@+id/editText8"
            android:layout_marginBottom="1dp" app:layout_constraintBottom_toTopOf="@+id/editText11"
            android:elegantTextHeight="false" android:editable="false"/>
    <com.google.android.material.textfield.TextInputEditText
            android:layout_width="129dp"
            android:layout_height="40dp"
            android:hint="Phone Number"
            android:id="@+id/editText10"
            android:background="@color/white" android:layout_marginTop="6dp"
