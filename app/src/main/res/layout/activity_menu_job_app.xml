            android:layout_width="277dp"
            android:layout_height="36dp"
            android:hint="ex:name@gmail.com"
            android:id="@+id/editText8"
            android:background="@color/white"
            android:layout_marginStart="12dp"
            app:layout_constraintStart_toEndOf="@+id/textView4" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/editText6" android:layout_marginBottom="6dp"
            app:layout_constraintBottom_toTopOf="@+id/editText9" android:layout_marginEnd="9dp"
            app:layout_constraintEnd_toEndOf="parent" android:editable="false"/>
    <com.google.android.material.textfield.TextInputEditText
            android:layout_width="266dp"
            android:layout_height="40dp"
            android:hint="Position"
            android:id="@+id/editText11"
            android:background="@color/white" app:layout_constraintTop_toBottomOf="@+id/editText10"
            app:layout_constraintBottom_toTopOf="@+id/editText12" android:layout_marginStart="13dp"
            app:layout_constraintStart_toEndOf="@+id/textView6" android:layout_marginEnd="9dp"
            app:layout_constraintEnd_toEndOf="parent" android:editable="false"/>
